PROJECT_NAME=Aspacelife OCR Text Detection API
DESCRIPTION=API for extracting text from images using Tesseract OCR
VERSION=1.0.0
CORS_ORIGINS=*
MAX_FILE_SIZE=10485760
ALLOWED_EXTENSIONS=png,jpg,jpeg,tiff,bmp,gif
UPLOAD_DIR=uploads
DEFAULT_LANGUAGE=eng
LOG_FILE=logs/app.log
KAFKA_BROKERS_URL=localhost:8000
KAFKA_TOPIC=ocr_results
KAFKA_LOG_LEVEL=DEBUG